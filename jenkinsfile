pipeline {
    agent any
    tools {
            maven 'Maven_3.5.2'
        }
    stages {
        stage('Source Code') {
          steps {
            git 'https://github.com/girish513/ActuatorExample'
            echo 'Get Code from GIT Repo'
          }
        }
        stage ('Compile Stage') {
            steps {
                    sh 'mvn clean compile'
                    echo 'Compile Code'
             }
         }
        stage ('Testing Stage') {
            steps {
                    sh 'mvn test'
                     echo 'Testing Code'
            }
        }
        stage ('Install Stage') {
             steps {
                    sh 'mvn install'
                     echo 'Install Code'
            }
        }
    }
}
